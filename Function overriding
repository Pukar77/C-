// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
class pukar{
    public:
    void display(){
        cout<<"I am base class "<<endl;
    }
};
class rimal: public pukar{
    public:
  void display(){           //function overriding
      cout<<"I am derived class "<<endl;
  }  
};
int main() {
    pukar p;
    p.display();    //[displays i am base class]
   
   rimal r;
   r.display();    //[displays i am derived class]
   
   
    return 0;      //[output is ]
                   //I am base class  
                   // am derived class

    
}
