#include <iostream>
using namespace std;
 
class construct
{
 
public:
    float a,b;
     
    // Constructor with no parameters
    construct()
    {
        a= 0;
        b=1;
    }
     
    // Constructor with two parameters
    construct(int a1, int b1)
    {
       a=a1;
       b=b1;
    }
     
    void disp()
    {
        cout<< a<<" "<<b<< endl;
    }
};
 
int main()
{

    construct o1;
    construct o2( 10, 20);
     
    o1.disp();
    o2.disp();
    return 0;
}
