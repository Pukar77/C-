// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
void pukar(int a, int b){
    int c;
    c=a+b;
    cout<<"Value after addition is "<<c<<endl;
}
void pukar(double a,double b){
    double c;
    c=a+b;
    cout<<"Value after addition is "<<c<<endl;
}
int main() {
    pukar(5,5);
    pukar(10.57,5.5);
   

    return 0;
}
