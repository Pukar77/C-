// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
inline float sum(int l, int b){
    float z;
    z=l*b;
    return z;
}
int main() {
    float l,b,z;
    cout<<"Enter the value of l and b "<<endl;
    cin>>l>>b;
    z=sum(l,b);
    cout<<"Area is "<<z;
   

    return 0;
}
